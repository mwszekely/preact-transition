{
  "name": "preact-transition",
  "type": "module",
  "version": "0.1.0",
  "description": "Easy management of CSS transition classes and in-place swappable content.",
  "source": "src/index.tsx",
  "module": "/dist/index.js",
  "types": "/dist/index.d.ts",
  "packageManager": "pnpm@8.5.1",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "npx npm-run-all --npm-path npm -sc build:sass build:ts",
    "watch": "npx npm-run-all --npm-path npm -pc watch:*",
    "watch:sass": "sass --watch --load-path=\"../node_modules\" ./src/index.scss:./dist/index.css",
    "watch:ts": "tsc --watch",
    "build:sass": "sass --load-path=\"../node_modules\" ./src/index.scss:./dist/index.css",
    "build:ts": "tsc --build",
    "test": "jest",
    "clean": "tsc --build --clean",
    "rebuild": "rm \"./pnpm-lock.yaml\" && rm -R \"./node_modules\" && pnpm install && pnpm run build"
  },
  "author": "",
  "license": "ISC",
  "peerDependencies": {
    "preact": "^10.16.0"
  },
  "typings": "./dist/index.d.ts",
  "devDependencies": {
    "@types/lodash": "^4.14.196",
    "@types/lodash-es": "^4.17.7",
    "npm-run-all": "^4.1.5",
    "sass": "^1.64.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "clsx": "^2.0.0",
    "lodash-es": "^4.17.21",
    "preact-prop-helpers": "github:mwszekely/preact-prop-helpers"
  }
}
